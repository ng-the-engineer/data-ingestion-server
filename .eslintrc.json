{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 144 }],
        "prefer-promise-reject-errors": "off"
    }
}
